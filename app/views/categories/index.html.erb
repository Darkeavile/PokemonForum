
<h1>Pokemon Forum </h1>
  <div class="row">
  <div id="faq" class="col-md-9">
      <div class="panel-group" id="accordion">
<% @categories.each do |category| %>
  <% @name = "category-#{category.id}"%>


        <div class="panel panel-default">
          <div class="panel-heading">
            <h4 class="panel-title">
              <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion"
                href='#<%= @name %>'><%= category.title %>
              </a>
            </h4>
          </div>
          <div id='<%= @name %>' class="panel-collapse collapse">
            <div class="panel-body">
			<table>
  <tr>
    <th width="70%">Forum</th>
    <th width="30%">Last Post</th>
  </tr>
      <tr>
      <td width="70%"><h4><%= render category.forum %></h4>
	  </td>
      <td class="right" width="30%">LPE</td>
	        <td>Last Post Example</td>
    
	  </tr></table>
            </div>
          </div>


<% end %>
        </div>
      </div>
    </div>